html, body {
  height: 100%;
}

body {
  display: flex;
  flex-direction: column;
}
.page-content {
  flex: 1 0 auto;
}
footer {
  flex-shrink: 0;
}

body {
    font-family: 'Poppins', sans-serif;
    color: #[body_text_color];
    line-height: 1.5;
    background: #fff;
    font-size: [base_font_size]px;
}
h1 {
    font-size: [heading_base_size]px;
}

.titles {
    margin: 0 0 20px;
    font-size: 18px;
    font-weight: 600;
    letter-spacing: 0;
}

.subcategories {
    border-bottom: 1px solid #ececec;
    padding: 0 0 30px;
}
.subcategories > ul > li > a {
    color: #000;
    text-decoration: none;
    display: block;
    -webkit-transition: all .25s ease-out;
    transition: all .25s ease-out;
}
.subcategories > ul > li > a:hover {
    opacity: 0.8;
}
.subcategories > ul > li span.name {
    color: #000;
    font-size: 14px;
    font-weight: 600;
    text-transform: uppercase;
}
.checkout-1 .cards {
    text-align: left;
}

.homepage-slider .flexslider .slides img {
    width: 100%;
}
.checkout-1 .copyright {
    width: 100%;
}
.checkout-1 .site-footer .col-md-6 {
    width: 100%;
}
.checkout-1 .footer-social ul {
    text-align: left;
}
.checkout-1 ul.payment-icons li.payment-icons-label, 
.checkout-1 ul.social-icons li.social-icons-label {
    color: #000;
}
.checkout-1 .global-footer {
    display: none;
}

@media (min-width: 1200px) {
    .container {
        width: 1200px;
    }
}
.form-control {
    color: #000;
    font-family: 'Poppins', sans-serif;
    font-size: 13px;
    height: 36px;
    padding: 7px 12px;
}
.form-control.search-text {
    border: 1px solid #ccc;
    box-shadow: none;
}

.btn {
    font-size: 13px;
    padding: 8px 12px;
    font-weight: 600;
    text-transform: uppercase;
}
.btn-default,
.btn-primary:focus, 
.btn-primary:active:focus {
    border-color: #[primary-btn-border];
    background-color: #[primary-btn-bg];
    color: #[primary-btn-text];
}
.btn-default:hover,
.btn-default:focus,
.btn-default:hover:focus,
.btn-default.focus {
    color: #[primary-btn-text-hover];
    background-color: #[primary-btn-bg-hover];
    border-color: #[primary-btn-border-hover];
}
.btn-default.btn-inverse,
.btn-primary {
    color: #[secondary-btn-text];
    background-color: #[secondary-btn-bg];
    border-color: #[secondary-btn-border];
}
.navbar-wrapper {
    background: #[secondary-accent];
}
.btn-default.btn-inverse:hover,
.btn-primary:hover {
    color: #[secondary-btn-text-hover];
    background-color: #[secondary-btn-bg-hover];
    border-color: #[secondary-btn-border-hover];
    opacity: 0.9;
}
.navbar.navbar-inverse {
    margin-bottom: 0;
    border: none;
    background-color: #[secondary-accent];
}
.navbar-inverse .navbar-nav > li > a {
    color: #[navigation-text-color];
    font-weight: 500;
    letter-spacing: 0.025em;
    text-transform: uppercase;
    font-size: [navigation-font-size]px;
    line-height: 18px;
    min-height: 50px;
    font-family: 'Poppins', sans-serif;
}
.navbar-inverse .navbar-nav > li > a:hover, .navbar-inverse .navbar-nav > li > a:focus {
    color: #[navigation-text-hover-color];
    background-color: #[navigation-text-hover-bg];
}
.dropdown-menu > li > a {
    font-size: 12px;
    padding: 6px 20px;
    text-transform: uppercase;
}
.navbar-nav > li.dropdown > a:after {
    content: "\f107";
    font-family: "fontello";
    font-size: 18px;
    line-height: 1;
    margin-left: 5px;
}
@media (max-width: 1023px) {
  .navbar-header {
      float: none;
  }
  .navbar-left,.navbar-right {
      float: none !important;
  }
  .navbar-toggle {
      display: block;
      background: #000;
  }
  .navbar-collapse.collapse {
      display: none!important;
  }
  .navbar-nav {
      float: none!important;
  }
  .navbar-nav>li {
      float: none;
  }
  .collapse.in{
      display:block !important;
  }
  .navbar-nav .open .dropdown-menu {
        position: static;
        float: none;
        width: auto;
        margin-top: 0;
        background-color: transparent;
        border: 0;
        -webkit-box-shadow: none;
        box-shadow: none;
    }
    .navbar-inverse .navbar-nav .open .dropdown-menu > li > a {
        color: #9d9d9d;
    }
    .navbar-nav .open .dropdown-menu > li > a {
        line-height: 20px;
    }
    .navbar-nav .open .dropdown-menu > li > a, .navbar-nav .open .dropdown-menu .dropdown-header {
        padding: 5px 15px 5px 25px;
    }
}
